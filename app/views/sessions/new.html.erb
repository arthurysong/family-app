<%= flash[:notice] %><br>

<%= link_to "Sign in via GitHub", "/auth/github" %>

<%= form_tag '/signin' do %>
    <%= label_tag 'email' %>
    <%= text_field_tag 'email' %>
    <br>

    <%= label_tag 'password' %>
    <%= password_field_tag 'password' %>
    <br>

    <%= submit_tag "Log In" %>
<% end %>